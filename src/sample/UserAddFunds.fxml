<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="ProfProfileAnchorPane" prefHeight="220.0" prefWidth="414.0" style="-fx-background-color: #0f1f2a;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.UserAddFundsController">
    <children>
        <Label fx:id="AddFundsLabel" alignment="CENTER" layoutX="58.0" layoutY="27.0" prefHeight="50.0" prefWidth="208.0" text="ADD FUNDS" textFill="#f5f2f2">
            <font>
                <Font name="DejaVu Sans Bold" size="36.0" />
            </font>
        </Label>
        <TextField fx:id="AmountField" layoutX="58.0" layoutY="130.0" onMouseClicked="#onTextFieldsClicked" prefHeight="25.0" prefWidth="252.0" />

        <Label fx:id="AmountLabel" layoutX="58.0" layoutY="105.0" prefHeight="25.0" prefWidth="56.0" text="Amount" textFill="WHITE" />

        <Button fx:id="AddButton" layoutX="58.0" layoutY="161.0" mnemonicParsing="false" onAction="#onAddButtonClicked" prefHeight="42.0" prefWidth="252.0" text="ADD" />
        <Button fx:id="logoutButton" layoutX="351.0" layoutY="2.0" mnemonicParsing="false" onAction="#onLogOutButtonClicked" text="LOGOUT" />
        <Button fx:id="backButton" layoutX="280.0" layoutY="2.0" mnemonicParsing="false" onAction="#onBackButtonClicked" prefHeight="25.0" prefWidth="56.0" text="BACK" />
        <Label fx:id="infAmountLabel" layoutX="252.0" layoutY="209.0" text="Label" visible="false" />
    </children>
</AnchorPane>
